{
	"family": "second",
	"networkMode": "awsvpc",
	"containerDefinitions": [
		{
		"name": "myapp",
		"image": "620669233735.dkr.ecr.ap-south-1.amazonaws.com/sample:latest",
		"portMappings": [
			{
			"containerPort": 8080,
			"hostPort": 8080,
